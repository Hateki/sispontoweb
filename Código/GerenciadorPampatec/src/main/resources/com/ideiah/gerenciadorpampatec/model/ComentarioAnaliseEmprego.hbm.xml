<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ideiah.gerenciadorpampatec.model.ComentarioAnaliseEmprego" table="comentario_analiseemprego">
            <id name="id_comentarioAnaliseEmprego" type="java.lang.Integer">
            <column name="id_comentarioAnaliseEmprego" />
            <generator class="identity" />        
        </id>
        <property name="relacoesClientes" type="string">
            <column name="relacoesClientes" length="1000" />
        </property>
        <property name="parceriasChaves" type="string">
            <column name="parceriasChaves" length="1000" />
        </property>
        <property name="canais" type="string">
            <column name="canais" length="1000" />
        </property>
        <property name="recursosprincipais" type="string">
            <column name="recursosPrincipais" length="1000" />
        </property>
        <property name="concorrentes" type="string">
            <column name="concorrentes" length="1000" />
        </property>
        <one-to-one name="comentarioProjeto" class="com.ideiah.gerenciadorpampatec.model.ComentarioProjeto"
                    cascade="save-update">
        </one-to-one>
    </class>
</hibernate-mapping>
