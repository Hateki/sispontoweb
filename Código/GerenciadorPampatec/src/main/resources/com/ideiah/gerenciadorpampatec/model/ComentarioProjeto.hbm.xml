<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ideiah.gerenciadorpampatec.model.ComentarioProjeto" table="comentario_projeto">
        <id name="id_comentarioProjeto" type="java.lang.Integer">
            <column name="id_comentarioProjeto" />
            <generator class="identity" />
        </id>
        <one-to-one name="comentario_AnaliseEmprego" class="com.ideiah.gerenciadorpampatec.model.ComentarioAnaliseEmprego"
                    cascade="save-update">
        </one-to-one>
        <one-to-one name="comentario_ProdutoOuServico" class="com.ideiah.gerenciadorpampatec.model.ComentarioProdutoOuServico"
                    cascade="save-update">
        </one-to-one>
        <one-to-one name="comentario_Negocio" class="com.ideiah.gerenciadorpampatec.model.ComentarioNegocio"
                    cascade="save-update">
        </one-to-one>
        <one-to-one name="comentario_PlanoFinanceiro" class="com.ideiah.gerenciadorpampatec.model.ComentarioPlanoFinanceiro"
                    cascade="save-update">
        </one-to-one>
        <property name="participacaoAcionaria" type="string">
            <column name="participacaoAcionaria" length="1000" />
        </property>
        <property name="potencialEmprego" type="string">
            <column name="potencialEmprego" length="1000" />
        </property>
        <property name="comentarioGeral" type="string">
            <column name="comentarioGeral" length="1000" />
        </property>
    </class>
</hibernate-mapping>
