<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ideiah.gerenciadorpampatec.model.EmpreendedorEmail" table="empreendedoremail"/>
    <id name="idEmpreendedorEmail" type="string">
        <column name="idEmpreendedorEmail" length="150"/>
        <generator class="identity" />
    </id>
    <property name="tipo" type="string">
        <column name="tipo" length="45"/>
    </property>
    <many-to-many entity-name="com.ideiah.gerenciadorpampatec.model.Empreendedor">
        <column name="Empreendedor_idEmpreendedor" not-null="true" />
    </many-to-many>
</hibernate-mapping>
