<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ideiah.gerenciadorpampatec.model.empreendedorEmail" table="empreendedorEmail"/>
    <id name="idEmpreendedorEmail" type="String">
        <column name="idEmpreendedor" length="150"/>
        <generator class="assigned" />
    </id>
    <property name="tipo" type="string">
        <column name="tipo" length="45" />
    </property>
    <many-to-one name="empreendedor" class="com.ideiah.gerenciadorpampatec.model.Empreendedor" fetch="select">
        <column name="empreendedor_idEmpreendedorEmail" />
    </many-to-one>
</class>
</hibernate-mapping>
