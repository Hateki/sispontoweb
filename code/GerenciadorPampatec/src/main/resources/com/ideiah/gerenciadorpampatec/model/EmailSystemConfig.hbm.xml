<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.ideiah.gerenciadorpampatec.model.EmailSystemConfig" table="emailsystemconfig">
        <id name="id_systememail" type="integer">
            <column name="id_systememail" />
            <generator class="increment" />
        </id>
        <property name="email" type="string">
            <column name="email" length="255"/>
        </property>
        <property name="passphrase" type="text">
            <column name="passphrase" length="512" />
        </property>
    </class>
</hibernate-mapping>
